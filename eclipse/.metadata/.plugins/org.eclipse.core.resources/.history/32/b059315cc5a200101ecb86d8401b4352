package net.minecraft.src;

public class mod_RecipeTest extends BaseMod
{
    public String Version()
    {
        return "RecipeTest";
    }
    
    public mod_Paintmod()
    {
        ModLoader.AddRecipe(new ItemStack(Item.stick, 1), new Object[] {
            "X", "X", Character.valueOf('X'), Block.dirt
        });
        System.out.println("IT DID TIS!!!!!!!!!!");
    }
    
}